{
  "accessibility.dimUnfocused.enabled": true,
  "catppuccin.accentColor": "rosewater",
  "catppuccin.customUIColors": {
    "mocha": {
      "commandCenter.border": "#30313E"
    }
  },
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorSurroundingLines": 8,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.cursorMoveOnType": false,
  "editor.fontFamily": "Jetbrains Mono, D2Coding Ligature",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.fontVariations": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "files.autoSave": "onFocusChange",
  "telemetry.telemetryLevel": "error",
  "todo-tree.general.tags": ["TODO:"],
  "todo-tree.highlights.backgroundColourScheme": ["orange"],
  "todo-tree.highlights.foregroundColourScheme": ["black"],
  "todo-tree.highlights.useColourScheme": true,
  "todo-tree.tree.labelFormat": "${after}",
  "window.autoDetectHighContrast": false,
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.settings.applyToAllProfiles": [
    "accessibility.dimUnfocused.enabled",
    "catppuccin.accentColor",
    "catppuccin.customUIColors",
    "editor.cursorSmoothCaretAnimation",
    "editor.cursorSurroundingLines",
    "editor.defaultFormatter",
    "editor.find.cursorMoveOnType",
    "editor.fontFamily",
    "editor.fontLigatures",
    "editor.fontSize",
    "editor.fontVariations",
    "editor.formatOnSave",
    "editor.guides.bracketPairs",
    "editor.stickyScroll.enabled",
    "editor.tabSize",
    "files.autoSave",
    "telemetry.telemetryLevel",
    "todo-tree.general.tags",
    "todo-tree.highlights.backgroundColourScheme",
    "todo-tree.highlights.foregroundColourScheme",
    "todo-tree.highlights.useColourScheme",
    "todo-tree.tree.labelFormat",
    "window.autoDetectHighContrast",
    "workbench.colorTheme",
    "workbench.iconTheme"
  ]
}

